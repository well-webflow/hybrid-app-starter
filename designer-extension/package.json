    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.8",
    "@mui/material": "^6.1.7",
    "@mui/styled-engine": "^6.1.7",
    "@mui/system": "^6.1.7",
    "@tailwindcss/vite": "^4.0.14",
